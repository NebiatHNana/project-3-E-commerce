mport { QUERY_SHIRTCATEGORY } from '../../utils/queries';\nimport { ADD_CART } from '../../utils/mutations';\nimport Auth from '../../utils/auth'\n// import Integer from 'Math'\nconst ShirtList = ({ shirts, title }) => {\n  const [allShirts, setShirts] = useState(shirts)\n  const [vari, setVar] = useState('shirtHoodie')\n  const [addToCart] = useMutation(ADD_CART)\n  // const categoryVar = \"shirt\"\n\n  const setShirt = (e) => {\n    e.preventDefault()\n    setVar('shirt')\n  }\n\n  const setHoodie = (e) => {\n    e.preventDefault()\n    setVar('hoodie')\n  }\n\n  const clearFilter = (e) => {\n    e.preventDefault()\n    setVar('shirtHoodie')\n  }\n\n  const { data } = useQuery(QUERY_SHIRTCATEGORY, {\n    variables: { category: vari }\n  })\n\n  useEffect(() => {\n      const filteredShirts = data?.findShirt || []\n      setShirts(filteredShirts)\n      console.log(filteredShirts)\n  })\n\n  const cartFunction = async (e) => {\n    e.preventDefault()\n    const name = e.target.parentNode.dataset.name\n    const cost = e.target.parentNode.dataset.price\n    const img = e.target.parentNode.dataset.img\n    const username = Auth.getProfile().data.name\n    const userID = Auth.getProfile().data._id\n    console.log('USERID: '. userID)\n    const qnty = parseInt(e.target.previousSibling.children[1].value)\n    console.log(typeof qnty)\n    try {\n      const addingToCart = await addToCart({\n        variables: {\n          itemName: name,\n          price: cost,\n          imgurl: img,\n          userName: username,\n          userID: userID,\n          qty: qnty\n        }\n      })\n      console.log(addingToCart)\n    }\n    catch (err) {\n      console.log(err)\n    }\n    console.log(Auth.getProfile().data._id)\n  }\n  return (\n    <div>\n      <h3>{title}</h3>\n      <button id='gettingShirt' onClick={setShirt}>Shirts</button>\n      <button id='gettingHoodie' onClick={setHoodie}>Hoodies</button>\n      <button id='getAll' onClick={clearFilter}>Show All</button>\n      <div id='displayShirts'>{allShirts &&\n        allShirts.map((shirt) => (\n          <div key={shirt._id} id={shirt._id} data-name={shirt.itemName} data-price={shirt.price} data-img={shirt.imgurl} className=\"shirt\">\n            <h4 className=\"shirt heading\">\n              {shirt.itemName} <br /></h4>\n            <span style={{ fontSize: '0.8rem' }}>\n              <img src={shirt.imgurl} alt={shirt.itemName}></img>\n              <p>Price: {shirt.price}</p>\n            </span>\n            <section key={shirt.id}><label>Qty: </label>\n              <select key={shirt.id}>\n                <option value={1}>1</option>\n                <option value={2}>2</option>\n                <option value={3}>3</option>\n                <option value={4}>4</option></select></section>\n            <button id={shirt._id} onClick={cartFunction} >Add to cart :shopping_trolley:</button>\n          </div>\n        ))}</div>\n    </div>\n  );\n};\nexport default ShirtList;","/Users/leedu/bootcamp/project/project-3-E-commerce/client/src/utils/queries.js",[],"/Users/leedu/bootcamp/project/project-3-E-commerce/client/src/OnCodeApp.js",["83"],"import React from 'react';\nimport { useState } from 'react';\nimport { ApolloClient, ApolloProvider, InMemoryCache } from '@apollo/client';\n\nimport Auth from './utils/auth';\n\n// contains css for header && footer\nimport './OnCodeApp.css';\n// import components\nimport Header from './components/Header/Header';\nimport Footer from './components/Footer/Footer';\nimport Home from './components/pages/Home/Home';\nimport AboutUs from './components/pages/AboutUs/AboutUs'\nimport Collection from './components/pages/Collection/Collection';\nimport Cart from './components/pages/Cart/Cart';\nimport Login from './components/pages/Login/Login'\nimport Signup from './components/pages/Login/Signup'\n\nconst client = new ApolloClient({\n  uri: '/graphql',\n  cache: new InMemoryCache(),\n});\n\nfunction OnCodeApp() {\n\n  const [page, setPage] = useState('home');\n  const handlePageRender = page => setPage(page);\n\n  const renderPage = () => {\n    switch (page) {\n      case 'home':\n        return <Home page={page} handlePageRender={handlePageRender} />;\n\n      case 'about-us':\n        return <AboutUs />\n\n      case 'collection':\n        return <Collection />\n\n      case 'cart':\n          return <Cart handlePageRender={handlePageRender} />\n\n      case 'login':\n        return <Login handlePageRender={handlePageRender} />\n\n        case 'sign-up':\n          return <Signup handlePageRender={handlePageRender} />\n\n      default:\n    }\n\n  }\n\n\n  return (\n    <ApolloProvider client={client}>\n      <main>\n        {/* NAVBAR */}\n        <Header page={page} handlePageRender={handlePageRender} />\n        {/* MAIN SECTION TO RERENDER */}\n        <section id='page'>          \n          {renderPage()}\n        </section>\n        {/* FOOTER */}\n        <Footer />\n      </main>\n    </ApolloProvider>\n  );\n}\nexport default OnCodeApp;","/Users/leedu/bootcamp/project/project-3-E-commerce/client/src/components/Header/Header.jsx",["84","85","86","87"],"import logo from '../../Assets/logo.png';\nimport cartIcon from '../../Assets/cartIcon.png';\nimport Auth from '../../utils/auth'\n\nexport default function Header({page, handlePageRender}) {\n\n  const logout = () => {\n    handlePageRender('home')\n\n    Auth.logout();\n  }\n\n    return (\n      <header className=\"nav\">\n        <nav className='nav-card'>\n          <h1><a onClick={() => handlePageRender('home')} href='#home'>On Code</a></h1>\n          <div><img className='logo' src={logo} alt='onCode Logo' /></div>\n          <ul className=\"nav-links\">\n            <li className=\"nav-item\"><a onClick={() => handlePageRender('about-us')} >About Us</a></li>\n            <li className=\"nav-item\"><a onClick={() => handlePageRender('collection')} >Collection</a></li>\n\n            {Auth.loggedIn() ? \n            <li className=\"nav-item\"><a onClick={logout}>Logout</a></li> \n            :\n            <li className=\"nav-item\"><a onClick={() => handlePageRender('login')}>Login</a></li>\n            }\n            \n            \n            <li className=\"nav-item\"><a href='#cart'><img className='cart-icon' onClick={() => handlePageRender('cart')} src={cartIcon} alt='Shopping Cart Icon' /></a></li>\n          </ul>\n        </nav>\n      </header>\n    )\n  }","/Users/leedu/bootcamp/project/project-3-E-commerce/client/src/components/Footer/Footer.jsx",["88","89"],"export default function Footer() {\n    return (\n        <div className=\"footer-card\">\n            <ul className=\"footer-links\">\n                <li className=\"footer-item\"><a href='https://github.com/DustinLee0'>Github</a></li>\n                <li className=\"footer-item\"><a>Email Us</a></li>\n                <li className=\"footer-item\"><a>Cart</a></li>\n            </ul>\n        </div>\n    )\n}","/Users/leedu/bootcamp/project/project-3-E-commerce/client/src/components/pages/Home/Home.jsx",[],"/Users/leedu/bootcamp/project/project-3-E-commerce/client/src/components/Shirts/DisplayShirts.js",[],"/Users/leedu/bootcamp/project/project-3-E-commerce/client/src/components/pages/Collection/Collection.jsx",["90"],"\n\nimport './Collection.css'\n\nimport DisplayShirts from '../../Shirts/DisplayShirts';\nimport Shirts from '../../Shirts/index'\n\n\n\nexport default function Collection() {\n    return (\n        // DISPLAY SHIRTS PAGE\n        // <ApolloProvider client={client}>\n            <div className=\"flex-column justify-flex-start min-100-vh\">\n                <DisplayShirts />\n\n            </div>\n        // </ApolloProvider>\n    )\n}","/Users/leedu/bootcamp/project/project-3-E-commerce/client/src/components/pages/AboutUs/AboutUs.jsx",[],"/Users/leedu/bootcamp/project/project-3-E-commerce/client/src/components/Subtotal/Subtotal.jsx",["91","92","93","94"],"import './Subtotal.css';\n\nexport default function Cart() {\n    return (\n        <section class=\"summary-card\" id=\"summary\">\n            <h3>Summary</h3>\n            <p>Subtotal: <a id='subtotal'>test</a> </p>\n            <p>Estimated Delivery and Handling: <a id='shipping'>test</a> </p>\n            <p>Taxes: <a id='tax'>test</a> </p>\n            <p>Total: <a id='total'>test</a> </p>\n            <button onClick={() => {\n                console.log('checkout btn clicked')\n                }} type=\"button\">Checkout</button>\n        </section>\n    )\n}","/Users/leedu/bootcamp/project/project-3-E-commerce/client/src/components/pages/Login/Signup.jsx",[],"/Users/leedu/bootcamp/project/project-3-E-commerce/client/src/components/pages/Login/Login.jsx",[],"/Users/leedu/bootcamp/project/project-3-E-commerce/client/src/components/pages/Cart/Cart.jsx",["95","96","97","98","99"],"import './Cart.css';\nimport trashIcon from '../../../Assets/trashIcon.png';\nimport Subtotal from '../../Subtotal/Subtotal';\nimport Login from '../Login/Login';\nimport Auth from '../../../utils/auth';\n\nimport { Navigate, useParams } from 'react-router-dom';\nimport {\n    useState,\n    useEffect\n    } from 'react';\n\nimport { useQuery } from '@apollo/client';\nimport { QUERY_CART } from '../../../utils/queries';\n\nimport { useMutation } from '@apollo/client';\nimport { DELETE_ITEM } from '../../../utils/mutations';\n\nexport default function Cart() {\n    // get user name from token\n    let user = Auth.getProfile().data.name;\n    console.log('USER: ', user)\n    \n    // query cart item by user's name from logged in token \n    const { data } = useQuery(QUERY_CART, {\n        variables: {\n            userName: user\n        }\n    });\n    \n    const cartItems = data?.findUserCart || [];\n    // console.log('CARTITEMS: ', cartItems)\n    const [cartData, setCartData] = useState(cartItems);\n\n    useEffect(() => { \n\n        const cartData = data?.findUserCart || [];\n        setCartData(cartData);\n    })\n\n    // renders user's cart based on query data\n    const showUserCartData = (cartItems) => {\n        console.log(cartItems)\n        return cartItems.map((item, index) => {\n            return (\n                <div className=\"card\" id={index}>\n                    <div className='img-container'>\n                        <img src={item.imgurl} alt={item.itemName} />\n                    </div>\n                    <div className='item-body'>\n                        <p>{item.itemName}</p>\n                        <p>{item.price}</p>\n                        <div><a><img id={item._id} onClick={deleteItem} className='trash-icon' src={trashIcon} /></a></div>\n                    </div>\n                </div>\n            )\n        })\n    }\n\n\n    const [deleteCartItem] = useMutation(DELETE_ITEM)\n\n    const deleteItem = async (e) => {\n        e.preventDefault();\n        const shirtID = e.target.id;\n        console.log(shirtID)\n        try {\n            const deleteCart = await deleteCartItem({\n              variables: { _id: shirtID }\n            })\n            console.log('DELETED CART ITEM: ', deleteCart)\n            setCartData(deleteCart);\n\n          }\n          catch (err) {\n            console.log(err)\n          }\n    }\n\n\n    const login = Auth.loggedIn();\n\n    return (\n        <section className='product-section'>\n            {login ?\n                <div>\n                    {showUserCartData(cartData)}\n                    \n                    <Subtotal />\n                </div> :\n                <Login />\n            }\n        </section>\n\n    )\n}","/Users/leedu/bootcamp/project/project-3-E-commerce/client/src/utils/mutations.js",[],"/Users/leedu/bootcamp/project/project-3-E-commerce/client/src/utils/auth.js",["100"],"// use this to decode a token and get the user's information out of it\nimport decode from 'jwt-decode';\n\n// create a new class to instantiate for a user\nclass AuthService {\n  // get user data from JSON web token by decoding it\n  getProfile() {\n    return decode(this.getToken());\n  }\n\n  // return `true` or `false` if token exists (does not verify if it's expired yet)\n  loggedIn() {\n    const token = this.getToken();\n    return token ? true : false;\n  }\n\n  getToken() {\n    // Retrieves the user token from localStorage\n    return localStorage.getItem('id_token');\n  }\n\n  login(idToken) {\n    // Saves user token to localStorage and reloads the application for logged in status to take effect\n    localStorage.setItem('id_token', idToken);\n    window.location.assign('/');\n  }\n\n  logout() {\n    // Clear user token and profile data from localStorage\n    localStorage.removeItem('id_token');\n    // this will reload the page and reset the state of the application\n    window.location.reload();\n  }\n}\n\nexport default new AuthService();\n",{"ruleId":"101","replacedBy":"102"},{"ruleId":"103","replacedBy":"104"},{"ruleId":"105","severity":1,"message":"106","line":33,"column":3,"nodeType":"107","endLine":33,"endColumn":12,"suggestions":"108"},{"ruleId":"109","severity":1,"message":"110","line":46,"column":17,"nodeType":"111","messageId":"112","endLine":46,"endColumn":35,"fix":"113"},{"ruleId":"114","severity":1,"message":"115","line":5,"column":8,"nodeType":"107","messageId":"116","endLine":5,"endColumn":12},{"ruleId":"117","severity":1,"message":"118","line":19,"column":38,"nodeType":"119","endLine":19,"endColumn":87},{"ruleId":"117","severity":1,"message":"118","line":20,"column":38,"nodeType":"119","endLine":20,"endColumn":89},{"ruleId":"117","severity":1,"message":"118","line":23,"column":38,"nodeType":"119","endLine":23,"endColumn":58},{"ruleId":"117","severity":1,"message":"118","line":25,"column":38,"nodeType":"119","endLine":25,"endColumn":83},{"ruleId":"117","severity":1,"message":"118","line":6,"column":45,"nodeType":"119","endLine":6,"endColumn":48},{"ruleId":"117","severity":1,"message":"118","line":7,"column":45,"nodeType":"119","endLine":7,"endColumn":48},